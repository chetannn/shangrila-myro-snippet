{
  "Create a new myro controller": {
    "prefix": "myro.controller",
    "body": [
      "Ext.define('$1Controller', {",
      "extend: 'Myro.Controller',",
      "requires: [",
      "'Myro.Base',",
      "'$1Store',",
      "'$1Window',",
      "],",
      "alias: 'controller.$2',",
      "searchPanelSelector: '#$3',",
      "gridPanelSelector: '#$4',",
      "windowSelector: '#$5',",
      "mainPanelSelector: '#$6',",
      "reportUrlKey: '$7',",
      "reportName: '$8',",
      "init: function(application) {",
      "var me = this;",
      "me.callParent(arguments);",
      "config = me.getDefaultEventsConfig();",
      "me.control(config)",
      "},",
      "createModel: function(opts) {",
      "return new $1.Model(opts)",
      "},",
      "createFormWindowObject: function(opts) {",
      "var opts = opts || {};",
      "opts.controller = new $1Controller();",
      "return new $1Window(opts);",
      "},",
      "getSearchParam: function() {",
      "var params = this.callParent(arguments);",
      "return  params;",
      "}",
      "}); $0"
    ],
    "description": "creates a new myro controller"
  },
  "Create a new myro store with model": {
    "prefix": "myro.store",
    "body": [
      "Ext.define('$1.Model', {",
      "extend: 'Myro.RestModel',",
      "alias: 'model.$2',",
