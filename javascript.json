      "'Myro.RestModel',",
      "],",
      "fields: [",
      "'$3', '$4', '$5', '$6', '$7', '$8',",
      "],",
      "proxy: {",
      "type: 'rest',",
      "url: 'api/$9',",
      "reader: {",
      "type: 'json',",
      "messageProperty: 'message',",
      "rootProperty: 'data'",
      "}",
      "}",
      "});",
      "Ext.define('$1Store', {",
      "extend: 'Ext.data.Store',",
      "alias: 'store.$9Store',",
      "model: '$1.Model'",
      "}); $0"
    ],
    "description": "creates a new myro store with model"
  }
}
